<h1>Listing users</h1>

<table>
  <tr>
    <th>Provider</th>
    <th>Uid</th>
    <th>Name</th>
    <th>Created at</th>
    <th>Updated at</th>
    <th>Token</th>
    <th>Token Secret</th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.provider %></td>
    <td><%= user.uid %></td>
    <td><%= user.name %></td>
    <td><%= user.created_at %></td>
    <td><%= user.updated_at %></td>
     <td><%= user.token %></td>
      <td><%= user.token_secret %></td>

  </tr>
<% end %>
</table>

<br />


